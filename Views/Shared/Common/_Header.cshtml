<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<title>@ViewBag.Title</title>
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
<link rel="shortcut icon" sizes="16x16" href="https://digitalm3.com.br/wp-content/themes/Digital-M3-2017/assets/img/favicon-m3-16.png">
<link rel="shortcut icon" sizes="32x32" href="https://digitalm3.com.br/wp-content/themes/Digital-M3-2017/assets/img/favicon-m3-32.png">
@Html.Partial("Common/_SEO")

<script type="text/javascript">
    window.onload = function () {
        var svr = document.createElement("div");
        svr.setAttribute("type", "hidden");
        svr.setAttribute("id", "svr");
        svr.setAttribute("value", "@Context.Server.MachineName");

        document.body.appendChild(svr);
    }
</script>
